export JDBC_DATABASE_URL=jdbc:postgresql://localhost:5432/postgres?user=gitpod

java -jar myapp.jar -Dagentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=8000

curl -H "Accept: application/json" -H "Content-type: application/json" -X POST -d '{"name":"test","password":"test"}' http://localhost:3000/reg
curl -H "Accept: application/json" -H "Content-type: application/json" -X POST -d '{"username":"test","password":"test"}' http://localhost:3000/authenticate
curl -H "Authorization: Bearer $jwttoken" http://localhost:3000/greeting

netstat -plunt |grep postgres

ps -ef | grep postgres
psql -h ~/pg/sockets postgres

postgres commands
\du    - list users.
\l     - list dbs
\c db-name - chose db
\dt    - list tables